@model SearchViewModel
@{ 
    var test = Model;
}
<form asp-action="search" asp-controller="vehicle" method="post">
    <div class="form-group">
        <label for="make">Make</label>
        <input type="text" class="form-control" id="make" name="make" value="@Model.Make">
    </div>
    <div class="form-group">
        <label for="model">Model</label>
        <input type="text" class="form-control" id="model" name="model" value="@Model.Model">
    </div>
    <div class="form-group">
        <label for="licenseNumber">License number</label>
        <input type="text" class="form-control text-uppercase" maxlength="6" id="licenseNumber" name="licenseNumber" value="@Model.LicenseNumber">
    </div>
    <button type="submit" class="btn btn-primary">Search</button>
</form>